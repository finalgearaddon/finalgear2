<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.finalgear" name="FinalGear" version="1.6" provider-name="Griffin">
        <requires>
                <import addon="xbmc.python" version="2.19.0" />
                <import addon="script.module.nanscrapers" version="0.5.5" />
                <import addon="script.module.python.koding.aio" version="0.9.6.6"/>
                <import addon="script.module.beautifulsoup" version="3.2.1"/>
                <import addon="script.module.requests" version="2.9.1" />
                <import addon="script.module.addon.common" version="2.0.0" />
                <import addon="script.module.xmltodict" version="0.9.0"/>
                <!--<import addon="script.module.web-pdb" version="1.0"/>-->
                <import addon="script.module.urlresolver" version="1.2.0"/>
        </requires>
        <extension point="xbmc.python.pluginsource" library="default.py">
                <provides>audio video</provides>
        </extension>
        <extension point="xbmc.service" library="service.py" start="[login|startup]">
              </extension>
        <extension point="xbmc.addon.metadata">
          <summary lang="en">FinalGear by Griffin_303</summary>
          <description lang="en">The author does not host or distribute any of the content displayed by this addon. The author does not have any affiliation with the content provider.</description>
          <platform>all</platform>
          <forum>http://www.noobsandnerds.com/support/forumdisplay.php?fid=253</forum>
          <website>www.noobsandnerds.com</website>
          <video_guide>y0w8zIH4lOY</video_guide>
          <video_preview>WdKIzajiBBI</video_preview>
        </extension>
</addon>

<addon id="repository.finalgear" name="FinalGear Repository" version="1.1" provider-name="Griffin">
	<extension point="xbmc.addon.repository" name="FinalGear Repository">
		<info compressed="false">http://raw.github.com/finalgearaddon/finalgear2/master/addons.xml</info>
		<checksum>http://raw.github.com/finalgearaddon/finalgear2/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/finalgearaddon/finalgear2/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>FinalGear Repository</summary>
		<description>Home of the Final Gear Addon</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
